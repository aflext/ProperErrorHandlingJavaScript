{
  "curly": true,
  "eqeqeq": true,
  "freeze": true,
  "futurehostile": true,
  "maxdepth": 3,
  "maxstatements": 20,
  "maxparams": 7,
  "funcscope": true,
  "esversion": 5,
  "forin": true,
  "latedef": true,
  "maxcomplexity": 15,
  "noarg": true,
  "nocomma": true,
  "nonbsp": true,
  "nonew": true,
  "undef": true,
  "unused": true,
  "singleGroups": true,
  "strict": "implied",
  "node": true,
  "browser": true,
  "mocha": true,
  "globals": {
    "Promise": true
  },
  "-W100": true
}
